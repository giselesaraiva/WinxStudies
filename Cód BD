create table usuario(

	id int auto_increment primary key,
    usuario varchar(50),
    senha varchar (30)

);
drop table progresso;
create table progresso(

	id int auto_increment primary key,
    pk_usuario int,
    pontuacaoMat int,
    pontuacaoPort int,
    pontuacaoLit int,
    pontuacaoBio int,
    pontuacaoHis int,
    pontuacaoGeo int,
    pontuacaoFis int,
    pontuacaoQui int,
    foreign key (pk_usuario) references usuario(id)

);
use testequestoes;
insert into usuario (usuario,senha) values ('loiraa','root');
select * from usuario;
select * from progresso;
